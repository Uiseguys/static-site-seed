  "author": "Denis Croitoru <dennis.croitoru@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build && npm run copy-lib",
